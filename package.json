{
	"name": "@onigoetz/ntr-expect-snapshot",
	"version": "0.4.0",
	"keywords": [
		"snapshot",
		"testing",
		"node",
		"test",
		"node:test",
		"expect"
	],
	"author": "St√©phane Goetz <onigoetz@onigoetz.ch>",
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "src/index.js",
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"build": "true",
		"test": "node --test test/*.test.js",
		"test:ci": "c8 --clean --reporter=text --reporter=lcov --reporter=html --include=src node --test test/*.test.js",
		"format": "biome format .",
		"check": "biome check ."
	},
	"dependencies": {
		"@jest/snapshot-utils": "30.1.2",
		"ci-info": "4.3.0",
		"pretty-format": "30.0.5"
	},
	"peerDependencies": {
		"expect": "^29 || ^30"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.4",
		"c8": "10.1.3",
		"expect": "30.1.2"
	},
	"packageManager": "yarn@4.10.3"
}
